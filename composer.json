{
    "name": "sonhaichu/framework",
    "description": "Boilerplate template for laravel package",
    "authors": [
        {
            "name": "sonhaichu",
            "email": "sonhaichu@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [],
        "psr-4": {
            "Package\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Package\\Test\\": "tests/"
        }
    },
    "require": {
        "webpress/analytics": "self.version",
        "webpress/category-manager": "self.version",
        "webpress/comment-manager": "self.version",
        "webpress/configuration": "self.version",
        "webpress/contact-manager": "self.version",
        "webpress/core": "self.version",
        "webpress/dynamic": "self.version",
        "webpress/media-manager": "self.version",
        "webpress/menu-manager": "self.version",
        "webpress/order-manager": "self.version",
        "webpress/payment": "self.version",
        "webpress/post-manager": "self.version",
        "webpress/product-manager": "self.version",
        "webpress/script-manager": "self.version",
        "webpress/search": "self.version",
        "webpress/seo": "self.version",
        "webpress/sitemap": "self.version",
        "webpress/tag-manager": "self.version",
        "webpress/user-manager": "self.version",
        "webpress/utility": "self.version",
        "webpress/view-model": "self.version",
        "webpress/email-config": "self.version"
    },

    "require-dev": {},
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
            ]
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --color=always"
        ],
        "test:watch": [
            "vendor/bin/phpunit-watcher watch"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage-html coverages"
        ]
    }
}
