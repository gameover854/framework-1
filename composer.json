{
    "name": "webpress/framework",
    "description": "A Package With All Webpress Package",
    "authors": [
        {
            "name": "sonhaichu",
            "email": "sonhaichu@gmail.com"
        }
    ],
    "autoload": {
        "classmap": [],
        "psr-4": {
            "VCComponent\\Laravel\\Framework\\": "src/app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "VCComponent\\Laravel\\Framework\\Test": "tests/"
        }
    },
    "require": {
        "php": "^7.2.5|8.0",
        "cviebrock/eloquent-sluggable": "^7.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^7.0",
        "laravel/socialite": "4.4.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "2.1",
        "nesbot/carbon": "2.32.0",

        "webpress/analytics": "^0.0.6",
        "webpress/category-manager": "^0.0.7",
        "webpress/comment-manager": "^0.0.5",
        "webpress/configuration": "^0.0.5",
        "webpress/contact-manager": "^0.0.8",
        "webpress/core": "0.1.1",
        "webpress/dynamic": "^1.0.3",
        "webpress/media-manager": "1.0.2",
        "webpress/menu-manager": "^0.1.1",
        "webpress/order-manager": "^0.0.11",
        "webpress/payment": "^0.0.3",
        "webpress/post-manager": "^0.0.8",
        "webpress/product-manager": "0.0.16",
        "webpress/script-manager": "^0.0.6",
        "webpress/search": "^0.0.3",
        "webpress/seo": "^0.0.5",
        "webpress/sitemap": "^0.0.5",
        "webpress/tag-manager": "^0.0.5",
        "webpress/user-manager": "^0.0.6",
        "webpress/utility": "^0.0.9",
        "webpress/view-model": "^0.0.6"
    },
    "require-dev": {
        "facade/ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.3",
        "phpunit/phpunit": "^8.4",
        "orchestra/testbench": "^5.0",
        "spatie/phpunit-watcher": "^1.21"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
            ]
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit --color=always"
        ],
        "test:watch": [
            "vendor/bin/phpunit-watcher watch"
        ],
        "coverage": [
            "vendor/bin/phpunit --coverage-html coverages"
        ]
    }
}
